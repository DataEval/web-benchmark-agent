# API密钥配置（必需）
# 获取方式: https://firecrawl.dev/dashboard
FIRECRAWL_API_KEY=your_firecrawl_api_key_here
# 获取方式: https://jina.ai/api-dashboard
JINA_API_KEY=your_jina_api_key_here
# 获取方式: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# LLM配置（可选）
# 使用的LLM模型，默认为gpt-4
LLM_MODEL=gpt-4
# 温度参数，控制输出随机性，越低越确定，范围0-1
LLM_TEMPERATURE=0.1
# LLM提供者，支持openai, azure
LLM_PROVIDER=openai

# 提取器配置（可选）
# 默认使用的提取器，支持firecrawl, jina, combined
DEFAULT_EXTRACTOR=firecrawl
# API请求超时时间(秒)
TIMEOUT=30
# 失败重试次数
RETRY_COUNT=3

# 输出配置（可选）
# 默认输出目录
OUTPUT_DIR=./output
# 是否保存中间结果
SAVE_INTERMEDIATE=true
# 是否保存原始HTML
SAVE_HTML=false

# 日志配置（可选）
# 日志级别: INFO, DEBUG, WARNING, ERROR
LOG_LEVEL=INFO 